extends base.pug
block style 
    style
        include ../static/style.css
        include ../static/con_style.css
block scripts 
    script(src='/static/index.js')
block content
    div.container
        h1 Contact Us!!
        form(action="/contact", method="post" class="myform")
            input(type="text" name="name" placeholder="Enter Your Name" class="myInp") 
            input(type="number" name="age" placeholder="Enter Your Age" class="myInp") 
            input(type="email" name="email" placeholder="Enter Your Email" class="myInp") 
            input(type="text" name="phone" placeholder="Enter Your Phone No." class="myInp") 
            input(type="text" name="desc" placeholder="Description" class="myInp") 
            button(class="btn")
                |Submit